<%= form_for @product do |f| %>
  <%= f.label :name %>:<br>
  <%= f.text_field :name %><br><br>

  <%= f.label :price %>:<br>
  <%= f.text_field :price %><br><br>

  <%= f.label :inventory_count %>:<br>
  <%= f.text_field :inventory_count %><br><br>

  <%= f.label :description %>:<br><br>
  <%= f.text_area :description %><br><br>

  <%= f.submit %>
<% end %>
